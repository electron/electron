{
  "env": {
    "browser": true,
    "mocha": true,
    "serviceworker": true
  },
  "globals": {
    "Bindings": true,
    "Components": true,
    "UI": true,
    "WebView": true
  },
  "plugins": [
    "import",
    "mocha"
  ],
  "rules": {
    "mocha/no-exclusive-tests": "error",
    "import/enforce-node-protocol-usage": ["error", "always"]
  }
}
