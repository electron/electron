{
  "problemMatcher": [
    {
      "owner": "clang",
      "fromPath": "src/out/Default/args.gn",
      "pattern": [
        {
          "regexp": "^(.+)[(:](\\d+)[:,](\\d+)\\)?:\\s+(warning|error):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}
